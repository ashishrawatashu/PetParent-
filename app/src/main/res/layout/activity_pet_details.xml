<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:background="@color/list_background"
    android:layout_height="match_parent"
    tools:context=".activty.PetDetailsActivity">


    <androidx.cardview.widget.CardView
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="?attr/actionBarSize"
        android:background="@color/whiteColor"
        tools:ignore="MissingConstraints">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal">

            <TextView
                android:id="@+id/reports_headline_TV"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/pet_details"
                android:textColor="#000"
                android:textSize="20sp"
                android:textStyle="bold" />

            <ImageView
                android:id="@+id/back_arrow_IV"
                android:padding="8dp"
                android:visibility="visible"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentRight="true"
                android:src="@drawable/back_arrow" />

        </RelativeLayout>

    </androidx.cardview.widget.CardView>

    <WebView
        android:id="@+id/webview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"/>
    <LinearLayout
        android:visibility="gone"
        android:id="@+id/addNewEntry"
        android:layout_width="180dp"
        android:layout_height="50dp"
        android:background="@drawable/oval"
        android:layout_below="@id/toolbar"
        android:orientation="horizontal"
        android:layout_marginTop="20dp"
        android:padding="10dp"
        android:layout_marginLeft="10dp">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="ADD NEW PETS"
            android:textColor="#fff"
            android:textSize="14sp"
            android:gravity="center"
            android:drawableRight="@drawable/ic_baseline_add_white"/>

    </LinearLayout>

    <androidx.cardview.widget.CardView
        android:layout_marginTop="5dp"
        android:id="@+id/petInfo_CV"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/toolbar">


        <RelativeLayout
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:layout_marginLeft="15dp"
            android:padding="5dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:id="@+id/pet_info_LL"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/petname"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/petId"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/petParent"/>

            </LinearLayout>


            <LinearLayout
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:id="@+id/double_dot_LL"
                android:layout_toRightOf="@id/pet_info_LL"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/double_Dot"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/double_Dot"/>

                <TextView
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:textStyle="bold"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black_color"
                    android:text="@string/double_Dot"/>

            </LinearLayout>


            <LinearLayout
                android:layout_toRightOf="@id/double_dot_LL"
                android:id="@+id/pet_all_info_ll"
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/pet_name_TV"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="5dp"
                        android:text="Rustam"
                        android:textColor="@color/grayColorCode"
                        android:textSize="15sp" />

                    <TextView
                        android:visibility="gone"
                        android:layout_marginLeft="8dp"
                        android:layout_toRightOf="@id/pet_name_TV"
                        android:id="@+id/pet_sex_TV"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/grayColorCode"
                        android:text="(Male)"/>

                </RelativeLayout>



                <TextView
                    android:id="@+id/pet_id_TV"
                    android:layout_marginTop="5dp"
                    android:textSize="15sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textColor="@color/grayColorCode"
                    android:text=""/>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">

                    <TextView
                        android:id="@+id/pet_owner_name_TV"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/grayColorCode"
                        android:text="Ashish Rawat"/>

                    <TextView
                        android:visibility="gone"
                        android:layout_marginLeft="8dp"
                        android:layout_toRightOf="@id/pet_owner_name_TV"
                        android:id="@+id/pet_owner_phone_no_TV"
                        android:layout_marginTop="5dp"
                        android:textSize="15sp"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/grayColorCode"
                        android:text="(735-104-040-97)"/>

                </RelativeLayout>

            </LinearLayout>
        </RelativeLayout>



    </androidx.cardview.widget.CardView>


    <View
        android:id="@+id/view_green_line"
        android:layout_width="match_parent"
        android:layout_height="8dp"
        android:layout_below="@id/petInfo_CV"
        android:background="@color/green_divider"/>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view_green_line"
        >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <androidx.cardview.widget.CardView
                android:id="@+id/clinic_test"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/view_clinicVisits_IV"
                        android:layout_centerInParent="true"
                        android:src="@drawable/viewreports_icon"
                        android:layout_width="50dp"
                        android:layout_height="0dp"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@+id/view_clinicVisits_IV"
                        app:layout_constraintTop_toTopOf="@+id/view_clinicVisits_IV"
                        app:layout_constraintBottom_toBottomOf="@+id/view_clinicVisits_IV"
                        app:layout_constraintEnd_toStartOf="@+id/view_clinicVisits_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/report_clinicVisits_TV"
                        android:text="@string/clinicVisits" />


                    <ImageView
                        app:layout_constraintTop_toTopOf="@+id/view_clinicVisits_IV"
                        app:layout_constraintStart_toEndOf="@+id/report_clinicVisits_TV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/view_clinicVisits_arrow"
                        android:layout_centerInParent="true"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/xray_test"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/view_xrayReport_IV"
                        android:src="@drawable/viewreports_icon"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@+id/view_xrayReport_IV"
                        app:layout_constraintTop_toTopOf="@+id/view_xrayReport_IV"
                        app:layout_constraintBottom_toBottomOf="@+id/view_xrayReport_IV"
                        app:layout_constraintEnd_toStartOf="@+id/view_xrayReport_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/report_xrayReport_TV"
                        android:text="@string/xrayReport" />


                    <ImageView
                        app:layout_constraintStart_toEndOf="@+id/report_xrayReport_TV"
                        app:layout_constraintBottom_toBottomOf="@+id/report_xrayReport_TV"
                        app:layout_constraintEnd_toEndOf="parent"
                        android:layout_alignParentEnd="true"
                        android:layout_toRightOf="@+id/report_xrayReport_TV"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/view_xrayReport_arrow"
                        android:layout_centerInParent="true"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/lab_test_report"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/view_labTestReport_IV"
                        android:src="@drawable/viewreports_icon"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@+id/view_labTestReport_IV"
                        app:layout_constraintTop_toTopOf="@+id/view_labTestReport_IV"
                        app:layout_constraintBottom_toBottomOf="@+id/view_labTestReport_IV"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/report_labTestReport_TV"
                        android:text="@string/labTestReport" />


                    <ImageView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="@+id/report_labTestReport_TV"
                        app:layout_constraintBottom_toBottomOf="@+id/report_labTestReport_TV"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/view_labTestReport_arrow"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>


            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/hospitalization_sugeries"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/view_Hospitalization_IV"
                        android:src="@drawable/viewreports_icon"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintEnd_toStartOf="@+id/view_Hospitalization_arrow"
                        app:layout_constraintStart_toEndOf="@+id/view_Hospitalization_IV"
                        app:layout_constraintTop_toTopOf="@+id/view_Hospitalization_IV"
                        app:layout_constraintBottom_toBottomOf="@+id/view_Hospitalization_IV"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/report_Hospitalization_TV"
                        android:text="@string/Hospitalization" />


                    <ImageView
                        app:layout_constraintTop_toTopOf="@+id/report_Hospitalization_TV"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="@+id/report_Hospitalization_TV"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/view_Hospitalization_arrow"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/last_prescription"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/last_prescription_IV"
                        android:src="@drawable/last_prescription"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintTop_toTopOf="@+id/last_prescription_IV"
                        app:layout_constraintBottom_toBottomOf="@+id/last_prescription_IV"
                        app:layout_constraintStart_toEndOf="@+id/last_prescription_IV"
                        app:layout_constraintEnd_toStartOf="@+id/last_prescription_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/last_prescription_TV"
                        android:text="@string/LastPrescription" />


                    <ImageView
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="10dp"
                        android:id="@+id/last_prescription_arrow"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/recent_visits"
                android:visibility="gone"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:background="@drawable/reports_field"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        android:layout_marginLeft="20dp"
                        android:layout_alignParentStart="true"
                        android:id="@+id/recent_visits_IV"
                        android:layout_centerInParent="true"
                        android:src="@drawable/recent_visit"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@+id/recent_visits_IV"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/recent_visits_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_centerInParent="true"
                        android:layout_width="250dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/recent_visits_TV"
                        android:text="@string/recentVisits"
                        android:layout_toRightOf="@id/recent_visits_IV"/>


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintStart_toEndOf="@+id/reson_of_visit_TV"
                        android:layout_alignParentEnd="true"
                        android:layout_toRightOf="@+id/recent_visits_TV"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/recent_visits_arrow"
                        android:layout_centerInParent="true"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/immunization_CV"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/imminization_IV"
                        android:src="@drawable/last_prescription"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@id/imminization_IV"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/immunization_chart_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/immunization_TV"
                        android:text="@string/immunizationChartLabel"/>


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/immunization_TV"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/immunization_chart_arrow"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/print_id_card"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">

                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/print_id_card_IV"
                        android:src="@drawable/print_id"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@id/print_id_card_IV"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/print_id_card_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/print_id_card_TV"
                        android:text="@string/printIdCard"/>


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toEndOf="@id/print_id_card_TV"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/print_id_card_arrow"
                        android:src="@drawable/forward_arrow"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>


                </androidx.constraintlayout.widget.ConstraintLayout>

            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:id="@+id/view_history"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="20dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="80dp">


                <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintStart_toStartOf="parent"
                        android:layout_marginLeft="20dp"
                        android:id="@+id/view_history_IV"
                        android:src="@drawable/view_history"
                        android:layout_width="50dp"
                        android:layout_height="wrap_content"/>

                    <TextView
                        app:layout_constraintStart_toEndOf="@+id/view_history_IV"
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintBottom_toBottomOf="parent"
                        app:layout_constraintEnd_toStartOf="@+id/view_history_arrow"
                        android:gravity="start"
                        android:paddingLeft="20dp"
                        android:textSize="18dp"
                        android:textColor="@color/black_color"
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/view_history_TV"
                        android:text="@string/view_history" />


                    <ImageView
                        app:layout_constraintTop_toTopOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
