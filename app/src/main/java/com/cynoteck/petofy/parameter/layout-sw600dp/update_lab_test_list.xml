<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginRight="10dp"
    android:layout_marginLeft="10dp"
    android:layout_marginTop="10dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">




    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_marginTop="5dp"
        android:layout_marginBottom="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginLeft="10dp"
        android:id="@+id/card_view"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/view_LL"
            app:layout_constraintStart_toStartOf="@id/card_view"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:layout_marginRight="10dp"
            android:gravity="end"
            app:layout_constraintTop_toTopOf="@id/card_view"
            android:orientation="vertical"
            android:layout_width="0dp"
            android:layout_height="wrap_content">

            <TextView
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:gravity="end"
                android:id="@+id/update_TV"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:text="Update"
                android:textColor="@color/colorPrimary"
                android:textSize="15sp"/>

            <View
                app:layout_constraintTop_toBottomOf="@id/update_TV"
                app:layout_constraintStart_toStartOf="@id/update_TV"
                app:layout_constraintEnd_toEndOf="@id/update_TV"
                android:layout_width="0dp"
                android:layout_height="2dp"
                android:background="@color/colorPrimary"/>

        </androidx.constraintlayout.widget.ConstraintLayout>


        <TextView
            android:layout_marginTop="8dp"
            android:id="@+id/requesting_Vet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:layout_marginRight="350dp"
            app:layout_constraintTop_toBottomOf="@id/view_LL"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="@id/card_view"
            android:textColor="@color/black_color"
            android:text="@string/requestingVet"
            android:textStyle="bold"/>

        <TextView
            android:id="@+id/top_point"
            app:layout_constraintStart_toEndOf="@id/requesting_Vet"
            app:layout_constraintTop_toTopOf="@id/requesting_Vet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/black_color"
            android:text=":"
            android:textStyle="bold"/>

        <TextView
            app:layout_constraintStart_toEndOf="@id/top_point"
            app:layout_constraintTop_toTopOf="@id/requesting_Vet"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:gravity="end"
            android:id="@+id/requesting_Vet_TV"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/grayColorCode"
            android:text="Fecal Exam" />


        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/vet_phone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/requesting_Vet_TV"
            app:layout_constraintStart_toStartOf="@id/requesting_Vet"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/vetPhone"
            android:textStyle="bold"
            android:textColor="@color/black_color"
            android:layout_marginRight="350dp"/>


        <TextView
            android:id="@+id/top_point_two"
            app:layout_constraintStart_toEndOf="@id/vet_phone"
            app:layout_constraintTop_toTopOf="@id/vet_phone"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/black_color"
            android:text=":"
            android:textStyle="bold"/>


        <TextView
            app:layout_constraintStart_toEndOf="@id/top_point_two"
            app:layout_constraintTop_toTopOf="@id/top_point_two"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:gravity="end"
            android:id="@+id/vet_phone_TV"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/grayColorCode"
            android:text="Fecal Exam" />


        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/visit_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/vet_phone_TV"
            app:layout_constraintStart_toStartOf="@id/requesting_Vet"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/visit_date"
            android:textStyle="bold"
            android:textColor="@color/black_color"
            android:layout_marginRight="350dp"/>


        <TextView
            android:id="@+id/top_point_three"
            app:layout_constraintStart_toEndOf="@id/visit_date"
            app:layout_constraintTop_toTopOf="@id/visit_date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/black_color"
            android:text=":"
            android:textStyle="bold"/>


        <TextView
            app:layout_constraintStart_toEndOf="@id/top_point_three"
            app:layout_constraintTop_toTopOf="@id/top_point_three"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:gravity="end"
            android:id="@+id/visit_date_TV"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/grayColorCode"
            android:text="Fecal Exam" />


        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/lab_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/visit_date_TV"
            app:layout_constraintStart_toStartOf="@id/visit_date"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/labType"
            android:textStyle="bold"
            android:textColor="@color/black_color"
            android:layout_marginRight="350dp"/>


        <TextView
            android:id="@+id/top_point_four"
            app:layout_constraintStart_toEndOf="@id/lab_type"
            app:layout_constraintTop_toTopOf="@id/lab_type"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/black_color"
            android:text=":"
            android:textStyle="bold"/>


        <TextView
            app:layout_constraintStart_toEndOf="@id/top_point_four"
            app:layout_constraintTop_toTopOf="@id/top_point_four"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:gravity="end"
            android:id="@+id/lab_type_TV"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/grayColorCode"
            android:text="Fecal Exam" />



        <TextView
            android:layout_marginTop="10dp"
            android:id="@+id/lab_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toBottomOf="@id/lab_type_TV"
            app:layout_constraintStart_toStartOf="@id/lab_type"
            app:layout_constraintEnd_toEndOf="parent"
            android:text="@string/labType"
            android:textStyle="bold"
            android:textColor="@color/black_color"
            android:layout_marginRight="350dp"/>


        <TextView
            android:id="@+id/top_point_five"
            app:layout_constraintStart_toEndOf="@id/lab_name"
            app:layout_constraintTop_toTopOf="@id/lab_name"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/black_color"
            android:text=":"
            android:textStyle="bold"/>


        <TextView
            app:layout_constraintStart_toEndOf="@id/top_point_five"
            app:layout_constraintTop_toTopOf="@id/top_point_five"
            app:layout_constraintEnd_toEndOf="@id/card_view"
            android:gravity="end"
            android:id="@+id/lab_name_TV"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:textSize="15sp"
            android:textColor="@color/grayColorCode"
            android:text="Fecal Exam" />




    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>
